  {{>breadcrumb}}
  
  
  <div class="grid-row">
    
    
    <div class="column-two-thirds">

    
    	<form class="form">
    
    		
    		<h1 class="form-title heading-xlarge">Sign in</h1>
    		
    
    	  <p class="lede">Sign in to get started.</p>
    		
    		
    		<div class="alert-info" role="alert">
      
          <span class="icon icon-cross" role="presentation"></span>
          
          <div class="alert-message">
            <p>Your password has expired.</p>
          </div>
        
        </div>
    
    
    		<div class="form-group">
    			<label class="form-label-bold" for="customer-reference">Email address</label>
    			<input class="form-control" id="email-address" type="email" value="selenium-test@kainos.com">
    		</div>
    
    
    		<div class="form-group">
    			<label class="form-label-bold" for="password">Enter your password</label>
    			<input class="form-control" id="password" type="password" value="Password1">
    		</div>
    		
    
        <div class="form-group">
          
          <button class="button" type="submit">Sign in</button>	
        		
          <p><a href="/forgotten-your-password/">Forgotten your password?</a></p>
        
        </div>
        
        
    	</form>
    	
	
    </div>
    
    
  </div>